#import <UIKit/UIKit.h>

/**
 *  @brief 头像视图，用于显示用户头像
 */
@interface HeadPicView : UIView

/**
*  @brief 设置头像点击事件
*
*  @param target 响应点击事件的对象
*  @param action 响应点击事件的方法
*/
- (void)setTarget:(id)target action:(SEL)action;

/**
 *  @brief 用户头像
 */
@property (nonatomic, strong) UIImage *headPic;

@end
